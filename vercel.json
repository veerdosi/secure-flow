{
  "builds": [
    {
      "src": "client/package.json", 
      "use": "@vercel/next"
    }, 
    {
      "src": "api/src/index.ts", 
      "use": "@vercel/node", 
      "config": {"maxDuration": 30}
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/src/index.ts"
    }
  ]
}
