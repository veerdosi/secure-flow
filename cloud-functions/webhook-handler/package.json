{
  "name": "secure-flow-webhook-handler", 
  "version": "1.0.0", 
  "private": true, 
  "engines": {"node": ">=18"}, 
  "main": "dist/index.js", 
  "scripts": {
    "build": "tsc", 
    "start": "node dist/index.js", 
    "dev": "functions-framework --target=webhookHandler --source=dist", 
    "deploy": "gcloud functions deploy webhookHandler --runtime nodejs18 --trigger-http --allow-unauthenticated --source ."
  }, 
  "dependencies": {
    "@google-cloud/functions-framework": "^3.3.0", 
    "@google-cloud/firestore": "^7.1.0", 
    "@google-cloud/vertexai": "^1.1.0", 
    "axios": "^1.6.2"
  }, 
  "devDependencies": {
    "typescript": "^5.3.3", 
    "@types/node": "^20.10.5"
  }
}
